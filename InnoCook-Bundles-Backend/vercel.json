{
  "version": 2,
  "builds": [
    {
      "src": "../innocook--boundles-nextjs-app/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "../innocook--boundles-nextjs-app/$1"
    }
  ]
}


  
  
  
