{
	"version": 2,
	"builds": [
	  {
		"src": "innocook--bundles-nextjs-app/next.config.js",
		"use": "@vercel/next"
	  },
	  {
		"src": "InnoCook-Bundles-Backend/server.js",
		"use": "@vercel/node"
	  }
	],
	"routes": [
	  {
		"src": "/api/(.*)",
		"dest": "InnoCook-Bundles-Backend/server.js"
	  },
	  {
		"src": "/(.*)",
		"dest": "innocook--bundles-nextjs-app/$1"
	  }
	]
  }
  